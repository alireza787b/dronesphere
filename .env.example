# DroneSphere Environment Configuration
APP_NAME=DroneSphere
APP_ENV=development
APP_DEBUG=true

# Server
SERVER_HOST=0.0.0.0
SERVER_PORT=8002

# Security
SECRET_KEY=your-secret-key-here-change-this

# Database
DATABASE_URL=postgresql+asyncpg://dronesphere:dronesphere_pass_dev@localhost:5432/dronesphere
REDIS_URL=redis://localhost:6379/0
RABBITMQ_URL=amqp://dronesphere:dronesphere_pass_dev@localhost:5672/dronesphere

# Drone
MAVSDK_SERVER_ADDRESS=localhost
MAVSDK_SERVER_PORT=50051
DRONE_CONNECTION_STRING=udp://:14540

# AI/LLM
AI_PROVIDER=ollama
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama2

# Safety
MAX_ALTITUDE_METERS=120
MAX_SPEED_MPS=15
MIN_BATTERY_PERCENT=20

# Logging
LOG_LEVEL=INFO
